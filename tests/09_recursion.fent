define factorial(var n) {
    if (n < 2) {
        return 1;
    }
    var prev = n - 1;
    var result = factorial(prev);
    return n * result;
}

define fibonacci(var n) {
    if (n < 2) {
        return n;
    }
    var n1 = n - 1;
    var n2 = n - 2;
    var fib1 = fibonacci(n1);
    var fib2 = fibonacci(n2);
    return fib1 + fib2;
}

define countdown(var n) {
    if (n < 1) {
        return 0;
    }
    var next = n - 1;
    return countdown(next);
}

var fact5 = factorial(5);
var fib7 = fibonacci(7);
var count = countdown(10);

