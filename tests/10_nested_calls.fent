define add(var a, var b) {
    return a + b;
}

define multiply(var a, var b) {
    return a * b;
}

define compute(var x, var y, var z) {
    var sum = add(x, y);
    var prod = multiply(sum, z);
    return prod;
}

var a = 5;
var b = 10;
var c = 2;

var result = compute(a, b, c);
var nested = add(multiply(a, b), multiply(b, c));
var deep = add(add(a, b), add(c, a));

return result;
