define iseven(var n) {
    var remainder = n % 2;
    return remainder == 0;
}

define sumupto(var n) {
    var sum = 0;
    var i = 1;
    while (i < n) {
        sum = sum + i;
        i = i + 1;
    }
    return sum;
}

define max(var a, var b) {
    if (a > b) {
        return a;
    } else {
        return b;
    }
}

define min(var a, var b) {
    if (a < b) {
        return a;
    } else {
        return b;
    }
}

define abs(var n) {
    if (n < 0) {
        return -n;
    } else {
        return n;
    }
}

