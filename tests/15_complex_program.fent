define iseven(const n) {
    const remainder = n % 2;
    return remainder == 0;
}

define sumupto(const n) {
    const sum = 0;
    const i = 1;
    while (i < n) {
        sum = sum + i;
        i = i + 1;
    }
    return sum;
}

define max(const a, const b) {
    if (a > b) {
        return a;
    } else {
        return b;
    }
}

define min(const a, const b) {
    if (a < b) {
        return a;
    } else {
        return b;
    }
}

define abs(const n) {
    if (n < 0) {
        return -n;
    } else {
        return n;
    }
}

